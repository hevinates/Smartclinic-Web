<script>
    let selectedTest = '';
    let selectedCondition = '';

    function selectTest() {
        const select = document.getElementById('testSelect');
        selectedTest = select.value;
        
        if (selectedTest) {
            document.getElementById('step1').classList.remove('active');
            document.getElementById('step2').classList.add('active');
        }
    }

    function selectCondition(condition) {
        selectedCondition = condition;
        
        // Button seÃ§imi gÃ¶rselleÅŸtir
        document.querySelectorAll('.condition-btn').forEach(btn => {
            btn.classList.remove('selected');
        });
        event.target.closest('.condition-btn').classList.add('selected');
        
        // Sonucu gÃ¶ster
        setTimeout(() => {
            showResult();
            document.getElementById('step2').classList.remove('active');
            document.getElementById('step3').classList.add('active');
        }, 300);
    }

    function showResult() {
        const resultData = getTestData(selectedTest, selectedCondition);
        const container = document.getElementById('resultContainer');
        
        container.innerHTML = `
            <div class="result-header">
                <div class="result-icon ${resultData.severity}">
                    <i class="fa-solid ${resultData.icon}"></i>
                </div>
                <div class="result-title">
                    <h4>${resultData.title}</h4>
                    <div class="test-name">${resultData.testName}</div>
                </div>
                <div class="criticality-badge ${resultData.severity}">
                    Kritiklik: %${resultData.criticality}
                </div>
            </div>

            <div class="result-section">
                <h5><i class="fa-solid fa-triangle-exclamation"></i> Belirtiler</h5>
                <ul>
                    ${resultData.symptoms.map(s => `<li>${s}</li>`).join('')}
                </ul>
            </div>

            <div class="result-section">
                <h5><i class="fa-solid fa-lightbulb"></i> OlasÄ± Nedenler</h5>
                <ul>
                    ${resultData.causes.map(c => `<li>${c}</li>`).join('')}
                </ul>
            </div>

            <div class="result-section">
                <h5><i class="fa-solid fa-heart"></i> Ã–neriler</h5>
                <ul>
                    ${resultData.recommendations.map(r => `<li>${r}</li>`).join('')}
                </ul>
            </div>

            <div class="normal-range">
                <strong>ðŸ“Š Normal DeÄŸer AralÄ±ÄŸÄ±:</strong> ${resultData.normalRange}
            </div>
        `;
    }

    function resetAssistant() {
        selectedTest = '';
        selectedCondition = '';
        document.getElementById('testSelect').value = '';
        document.getElementById('step3').classList.remove('active');
        document.getElementById('step2').classList.remove('active');
        document.getElementById('step1').classList.add('active');
    }
</script>

